<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <title></title>
    
    @foreach(var styles in @Model.staticContent.styles) {
        <link href="@styles" type="text/css" rel="stylesheet" />
    }
</head>
<body>
    <div id="react-content">@Model.prerenderedContent</div>
    @foreach(var script in @Model.staticContent.scripts) {
        <script src="@script"></script>
    }
</body>
</html>